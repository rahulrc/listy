{
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true,
		"target": "ES2020",
		"module": "ESNext",
		"moduleResolution": "bundler",
		"isolatedModules": true
	},
	"include": ["src/**/*"],
	"exclude": ["node_modules/*", ".svelte-kit/*"]
}
